<!--index.vtl-->
<h1>Keep track of your travels!</h1>

<p>Where have you travelled?</p>

<form action="/travel" method="post">
  <label for="location">Travel location</label>
  <input id="location" name="location" type="text">
  <label for="description">Travel Description</label>
  <input id="description" name="description" type="text">

  <button type="submit" class="btn btn-default">Add Travel location</button>
</form>
<!--index.vtl-->
